{
    "name": "hoanglek13/passport-mongodb",
    "description": "Laravel Passport provides OAuth2 server support to Laravel and MongoDB.",
    "keywords": ["laravel", "passport", "oauth", "hoanglek13", "passport-mongodb"],
    "license": "MIT",
    "authors": [
        {
            "name": "Le The Hoang",
            "email": "hoangle.dev@vndev.info"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "firebase/php-jwt": "~3.0|~4.0",
        "guzzlehttp/guzzle": "~6.0",
        "illuminate/auth": "~5.4",
        "illuminate/console": "~5.4",
        "illuminate/container": "~5.4",
        "illuminate/contracts": "~5.4",
        "illuminate/database": "~5.4",
        "illuminate/encryption": "~5.4",
        "illuminate/http": "~5.4",
        "illuminate/support": "~5.4",
        "league/oauth2-server": "^6.0",
        "symfony/psr-http-message-bridge": "~1.0",
        "zendframework/zend-diactoros": "~1.0",
        "phpseclib/phpseclib": "^2.0"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Hoanglek13\\PassportMongoDB\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
